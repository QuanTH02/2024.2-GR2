public class MathUtils {
    public static boolean isPrime(int n) {
        if (n < 2) return false;
        for (int i = 2; i < n; i++) {  // Bug: should be i <= Math.sqrt(n)
            if (n % i == 0) return false;
        }
        return true;
    }
}
